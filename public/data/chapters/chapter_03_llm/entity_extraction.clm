abstract:
  purpose: Extract structured data from text using LLM
  description: 'Uses Ollama to extract entities from text and return them in JSON
    format.

    Demonstrates structured output capability.

    '
  inputs:
    text:
      type: string
      description: Text to extract entities from
  outputs:
    entities:
      type: object
      description: JSON object with extracted entities
  preconditions:
  - Ollama service is running
  postconditions:
  - Returns valid JSON
concrete:
  runtime: llm
  provider: ollama
  model: gemma3:latest
  llm_config:
    system_prompt: "You are an entity extraction system. Given text, extract named\
      \ entities \nand return them as a JSON object with the following structure:\n\
      {\n  \"people\": [\"list of person names\"],\n  \"places\": [\"list of location\
      \ names\"],\n  \"organizations\": [\"list of organization names\"],\n  \"concepts\"\
      : [\"list of key concepts\"]\n}\nReturn ONLY the JSON object, no other text.\n"
    temperature: 0.1
    max_tokens: 500
    response_format: json
  input_type: mcard
  output_type: mcard
  process_type: custom
balanced:
  test_cases:
  - given: "Apple Inc. announced that CEO Tim Cook will visit the new headquarters\
      \ \nin Cupertino, California next week to discuss artificial intelligence \n\
      initiatives with the research team.\n"
    when:
      operation: extract
    then:
      result_type: object
  expectations:
    performance:
      max_time_ms: 60000
    quality:
      structured: true
